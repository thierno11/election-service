[run]
source = .
omit =
    env/*
    tests/*
    __pycache__/*
    */migrations/*
    conftest.py
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:

[html]
directory = htmlcov